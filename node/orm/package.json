{
  "name": "orm-less",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "npm run db:reset && mocha --exit test/**/test.js",
    "db:create-schema-migration": "npx knex --knexfile db/db-agent-configuration.js migrate:make $migrationname",
    "db:create-data-insertion": "npx knex --knexfile db/db-agent-configuration.js seed:make $seedname",
    "db:reset": "npm run db:reset-schema && npm run db:schema && npm run db:data",
    "db:reset-schema": "node db/reset-schema.js",
    "db:schema": "knex --knexfile db/db-agent-configuration.js migrate:latest",
    "db:data": "knex --knexfile db/db-agent-configuration.js  seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bookshelf": "^1.2.0",
    "knex": "^0.21.0",
    "pg": "^8.0.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.2.0"
  }
}
