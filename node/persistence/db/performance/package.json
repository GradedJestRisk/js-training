{
  "name": "performance",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "execute-queries-cli": "node index.js",
    "get:foo": "curl -X GET http://localhost:3000/foo/short-query -H 'X-Correlation-ID:1'",
    "load-test": "npm-run-all --serial load-test:*",
    "load-test:a": "artillery run --config ./test/load/config/common.yml --environment local ./test/load/short-queries.yml",
    "load-test:b": "artillery run --config ./test/load/config/common.yml --environment local ./test/load/medium-queries.yml",
    "load-test:c": "artillery run --config ./test/load/config/common.yml --environment local ./test/load/long-queries.yml",
    "start-server": "npm run database:start && npm run database:create-schema && node server.js",
    "start": "npm run database:start && npm run database:create-schema && npm run execute-queries && npm run display-statistics",
    "database:start": "docker-compose --env-file ./.env --file ./database/docker-compose.yml up --detach && sleep 1",
    "database:create-schema": "node ./database/initialize.js",
    "display-statistics": "psql $MONITORING_DATABASE_URL -c 'SELECT * FROM query_statistics' ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "@hapi/hapi": "^20.1.2",
    "artillery": "^1.7.2",
    "artillery-plugin-expect": "^1.3.0",
    "axios": "^0.21.1",
    "blipp": "^4.0.2",
    "dotenv": "^8.2.0",
    "joi": "^17.4.0",
    "knex": "^0.95.4",
    "laabr": "^6.1.3",
    "newline-remove": "^1.0.2",
    "node-sql-parser": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "performance-now": "^2.1.0",
    "pg": "^8.6.0",
    "uuid": "^2.0.3"
  }
}
